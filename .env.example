# Database
DATABASE_URL=postgresql://username:password@localhost:5432/ai_platform
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=7d

# Cloud Providers
COREWEAVE_API_KEY=your-coreweave-api-key
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
GCP_PROJECT_ID=your-gcp-project-id
GCP_CREDENTIALS_PATH=path/to/gcp-credentials.json

# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# App Configuration
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Terraform
TF_VAR_coreweave_api_key=${COREWEAVE_API_KEY}
TF_VAR_aws_access_key=${AWS_ACCESS_KEY_ID}
TF_VAR_aws_secret_key=${AWS_SECRET_ACCESS_KEY}

# n8n Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your-n8n-password

# Monitoring
LOG_LEVEL=info
SENTRY_DSN=your-sentry-dsn-for-error-tracking