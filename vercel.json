{
  "version": 2,
  "builds": [
    {
      "src": "InfraView/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "cd InfraView && npm install",
        "buildCommand": "cd InfraView && npm run build:vercel",
        "outputDirectory": "InfraView/dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}